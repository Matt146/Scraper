<script>
window.onscroll = function() {myFunction()};

var stickytopbar = document.getElementById("stickytopbar");
var sticky = stickytopbar.offsetTop;
var navDropdown= document.getElementById("navbar");

function myFunction() {

  if (window.pageYOffset >= sticky) {
      if (navDropdown.classList.contains('-open')) {
			stickytopbar.classList.add("sticky-dropdown")
      }
	  stickytopbar.classList.add("sticky");
  } else {
	  stickytopbar.classList.remove("sticky");
      if(!(navDropdown.classList.contains('-open'))){
          stickytopbar.classList.remove("sticky-dropdown")
	  }
  }
}
</script>
  


    






